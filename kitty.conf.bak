font_family      VictorMono Nerd Font Mono Medium
bold_font        VictorMono Nerd Font Mono SemiBold
italic_font      VictorMono Nerd Font Mono Medium Oblique
bold_italic_font VictorMono Nerd Font Mono SemiBold Oblique
font_size 16.0

tab_bar_style powerline
tab_powerline_style angled

background_opacity 0.99

hide_window_decorations yes
draw_minimal_borders yes
initial_window_width  1200
initial_window_height 800
remember_windows_size no
window_padding_width 16 32

sync_to_monitor yes
repaint_delay 8
input_delay 1
macos_colorspace displayp3

include ./theme.conf
